{
	"$schema": "../node_modules/@tauri-apps/cli/schema.json",
	"build": {
		"withGlobalTauri": true,
		"beforeBuildCommand": "",
		"beforeDevCommand": "",
		"devPath": "../build",
		"distDir": "../+bundle"
	},
	"package": {
		"productName": "Turf",
		"version": "0.1.0"
	},
	"tauri": {
		"allowlist": {
			"all": true,
			"dialog": {
				"open": true,
				"message": true
			},
			"fs": {
				"all": true,
				"scope": [
					"*"
				]
			}
		},
		"bundle": {
			"active": true,
			"category": "DeveloperTool",
			"copyright": "",
			"deb": {
				"depends": []
			},
			"externalBin": [],
			"icon": [
				"icons/32x32.png",
				"icons/128x128.png",
				"icons/128x128@2x.png",
				"icons/icon.icns",
				"icons/icon.ico"
			],
			"identifier": "cx.turf.app",
			"longDescription": "",
			"macOS": {
				"entitlements": null,
				"exceptionDomain": "",
				"frameworks": [],
				"providerShortName": null,
				"signingIdentity": null
			},
			"resources": [],
			"shortDescription": "",
			"targets": "all",
			"windows": {
				"certificateThumbprint": null,
				"digestAlgorithm": "sha256",
				"timestampUrl": ""
			}
		},
		"security": {
			"csp": null
		},
		"updater": {
			"active": false
		},
		"windows": [{
			"fullscreen": false,
			"height": 600,
			"resizable": true,
			"title": "Turf",
			"width": 800
		}]
	}
}